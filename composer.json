    "require": {
        "php-di/php-di": "^6.0",
        "zendframework/zend-diactoros": "^2.1",
        "nikic/fast-route": "^1.0",
        "middlewares/utils": "^2.1",
        "middlewares/request-handler": "^1.4",
        "filp/whoops": "^2.3",
        "digitalnature/php-ref": "^1.2",
        "narrowspark/http-emitter": "^0.7.0",
        "oscarotero/middleland": "^1.0",
        "twig/twig": "^2.0",
        "twig/extensions": "^1.5",
        "usmanhalalit/pixie": "2.*@dev",
        "nette/finder": "^2.5",
        "aura/auth": "^2.0",
        "slowprog/composer-copy-file": "~0.3",
        "aptoma/twig-markdown": "~2.0",
        "michelf/php-markdown": "~1.3",
        "codezero/cookie": "~1.0"
    },
    "autoload": {
        "psr-4" : {

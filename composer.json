{
    "name": "vukbgit/simplex",
    "type": "library",
    "description": "Simplex",
    "license": "MIT",
    "authors": [
       {
            "name": "vuk"
       }
    ],
    "require": {
        "php-di/php-di": "^6.0",
        "laminas/laminas-diactoros": "^2.11",
        "nikic/fast-route": "^1.0",
        "middlewares/utils": "^3.3",
        "middlewares/request-handler": "^2.0",
        "filp/whoops": "^2.3",
        "kint-php/kint": "^3.2",
        "narrowspark/http-emitter": ">=2.0",
        "oscarotero/middleland": "^1.0",
        "twig/twig": "^2.0",
        "twig/extensions": "^1.5",
        "twig/intl-extra": "v3.4",
        "usmanhalalit/pixie": "2.*@dev",
        "nette/finder": "^2.5",
        "aura/auth": "^4.0",
        "slowprog/composer-copy-file": "~0.3",
        "aptoma/twig-markdown": "~2.0",
        "michelf/php-markdown": "~1.3",
        "codezero/cookie": "~1.0",
        "cocur/slugify": "~3.2",
        "spatie/image": "~1.7.2",
        "pschultz/php-bytes": "^1.0",
        "gabrielelana/byte-units": "^0.5.0",
        "laminas/laminas-captcha": "^2.8",
        "laminas/laminas-validator": "^2.12",
        "laminas/laminas-session": "^2.8",
        "box/spout": "^3.0",
        "phpmailer/phpmailer": "^6.0",
        "hackzilla/password-generator": "^1.4"
    },
    "autoload": {
        "psr-4" : {
            "Simplex\\" : "src/"
        },
        "files": [
            "src/functions.php",
            "src/global-functions.php"
        ]
    },
    "bin": [
        "bin/npm.sh",
        "bin/yarn.sh",
        "bin/sass.sh",
        "bin/git-setup-dev.sh",
        "bin/git-setup-prod.sh",
        "bin/update-all.sh",
        "bin/git-push-all.sh",
        "bin/clean-cache.sh"
    ]
}
